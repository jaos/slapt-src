#, c-format
msgid "%s - A SlackBuild utility\n"
msgstr "%s - Un utilitaire pour SlackBuild\n"

#, c-format
msgid "%s version %s\n"
msgstr "%s version %s\n"

#, c-format
msgid "Abort.\n"
msgstr "Annulé.\n"

#, c-format
msgid "Cached\n"
msgstr "En cache\n"

msgid "Do you want to continue? [y/N] "
msgstr "Voulez-vous continuer ? [y (oui) / N (non)] "

#, c-format
msgid "Done\n"
msgstr "Réalisé\n"

#, c-format
msgid "Download failed: %s\n"
msgstr "Erreur de téléchargement : %s\n"

#, c-format
msgid "Failed\n"
msgstr "Raté\n"

#, c-format
msgid "Failed to chdir to %s\n"
msgstr "Impossible de changer de répertoire pour %s\n"

#, c-format
msgid "Failed to open %s for reading\n"
msgstr "Impossible d'ouvrir %s en lecture\n"

#, c-format
msgid "Fetching %s..."
msgstr "Récupération de %s..."

#, c-format
msgid "Fetching slackbuild list from %s..."
msgstr "Récupération de la liste des slackbuild depuis %s..."

#, c-format
msgid "MD5SUM mismatch for %s\n"
msgstr "Les sommes de contrôle (md5sum) ne correspondent pas pour %s\n"

#, c-format
msgid "Mismatch between download files and md5sums\n"
msgstr "Incohérence entre les fichiers téléchargés et les sommes de contrôle (md5sum)\n"

#, c-format
msgid "Missing slackbuild: %s requires %s\n"
msgstr "Dépendances manquantes pour le slackbuild : %s nécessite %s\n"

#, c-format
msgid "SlackBuild Category: %s\n"
msgstr "Catégorie du SlackBuild : %s\n"

#, c-format
msgid "SlackBuild Description: %s\n"
msgstr "Description du SlackBuild : %s\n"

#, c-format
msgid "SlackBuild Files:\n"
msgstr "Fichiers du SlackBuild :\n"

#, c-format
msgid "SlackBuild Name: %s\n"
msgstr "Nom du SlackBuild : %s\n"

#, c-format
msgid "SlackBuild Requires: %s\n"
msgstr "Dépendances du SlackBuild : %s\n"

#, c-format
msgid "SlackBuild Version: %s\n"
msgstr "Version du SlackBuild : %s\n"

#, c-format
msgid "Unable to find all specified slackbuilds.\n"
msgstr "Impossible de trouver tous les slackbuilds spécifiés.\n"

# "update local cache of remote slackbuilds"
# and
# "list available slackbuilds"
# have not been gettextified!
#, c-format
msgid "Usage: %s [action]\n"
msgstr "Utilisation : %s [action]\n"

#, c-format
msgid "Usage: %s [option(s)] [action] [slackbuild(s)]\n"
msgstr "Utilisation : %s [option(s)] [action] [slackbuild(s)]\n"

# "The following packages will be %s:\n" has not been gettextified!
msgid "built"
msgstr "construits"

msgid "do not look for dependencies"
msgstr "ne pas rechercher les dépendances"

msgid "do not prompt"
msgstr "ne pas confirmer"

msgid "fetch, build, and install the specified slackbuild(s)"
msgstr "récupère, construit et installe les slackbuilds spécifiés"

msgid "fetched"
msgstr "récupérés"

msgid "installed"
msgstr "installés"

msgid "only fetch and build the specified slackbuild(s)"
msgstr "uniquement récupère et construit les slackbuilds spécifiés"

msgid "only fetch the specified slackbuild(s)"
msgstr "uniquement récupère les slackbuilds spécifiés"

msgid "search available slackbuilds"
msgstr "rechercher les slackbuilds disponibles"

msgid "show specified slackbuilds"
msgstr "afficher les slackbuilds spécifiés"

msgid "use the specified configuration file"
msgstr "utiliser le fichier de configuration spécifié"

