#, c-format
msgid "%s - A SlackBuild utility\n"
msgstr "%s - Ένα εργαλείο για αρχεία SlackBuild\n"

#, c-format
msgid "%s version %s\n"
msgstr "%s έκδοση %s\n"

#, c-format
msgid "Abort.\n"
msgstr "Ακύρωση.\n"

#, c-format
msgid "Cached\n"
msgstr "Στη μνήμη\n"

msgid "Do you want to continue? [y/N] "
msgstr "Θέλετε να συνεχίσετε; [y/N]"

#, c-format
msgid "Done\n"
msgstr "Έτοιμο\n"

#, c-format
msgid "Download failed: %s\n"
msgstr "Η λήψη του αρχείου απέτυχε: %s\n"

#, c-format
msgid "Failed\n"
msgstr "Αποτυχία\n"

#, c-format
msgid "Failed to chdir to %s\n"
msgstr "Αποτυχία αλλαγής καταλόγους στο %s\n"

msgid "Failed to chdir to build directory"
msgstr "Αδυναμία αλλαγής τρέχοντος καταλόγου στο κατάλογο εργασίας"

#, c-format
msgid "Failed to construct command string\n"
msgstr "Αδυναμία δημιουργίας εντολής\n"

#, c-format
msgid "Failed to create build directory [%s]\n"
msgstr "Αδυναμία δημιουργίας του καταλόγου εργασίας [%s]\n"

#, c-format
msgid "Failed to open %s for reading\n"
msgstr "Αποτυχία κατά το άνοιγμα του %s για ανάγνωση\n"

#, c-format
msgid "Failed to open current directory\n"
msgstr "Αδυναμία ανοίγματος του τρέχοντος καταλόγου\n"

#, c-format
msgid "Failed to read %s\n"
msgstr "Αδυναμία ανάγνωσης %s\n"

#, c-format
msgid "Fetching %s..."
msgstr "Λήψη του %s..."

#, c-format
msgid "Fetching slackbuild list from %s..."
msgstr "Λήψη της λίστας slackbuild από το %s..."

#, c-format
msgid "MD5SUM mismatch for %s\n"
msgstr "Αποτυχία στον έλεγχο MD5SUM για το %s\n"

#, c-format
msgid "Mismatch between download files and md5sums\n"
msgstr "Τα ληφθέντα αρχεία και τα αρχεία md5sum δεν ταιριάζουν\n"

#, c-format
msgid "Missing slackbuild: %s requires %s\n"
msgstr "Αδυναμία εύρεσης του slackbuild: το %s απαιτεί το %s\n"

#, c-format
msgid "No BUILDDIR specified in the configuration\n"
msgstr "Δεν έχει προσδιοριστεί το BUILDDIR στις ρυθμίσεις\n"

msgid "Options"
msgstr "Επιλογές"

#, c-format
msgid "SlackBuild Category: %s\n"
msgstr "Κατηγορία SlackBuild: %s\n"

#, c-format
msgid "SlackBuild Description: %s\n"
msgstr "Περιγραφή SlackBuild: %s\n"

#, c-format
msgid "SlackBuild Files:\n"
msgstr "Αρχεία SlackBuild:\n"

#, c-format
msgid "SlackBuild Name: %s\n"
msgstr "Όνομα SlackBuild: %s\n"

#, c-format
msgid "SlackBuild Requires: %s\n"
msgstr "Εξαρτήσεις Slackbuild: %s\n"

#, c-format
msgid "SlackBuild Version: %s\n"
msgstr "Έκδοση SlackBuild: %s\n"

#, c-format
msgid "The following dependent slackbuilds will be built and installed:\n"
msgstr "Οι ακόλουθες slackbuild εξαρτήσεις θα δημιουργηθούν και θα εγκατασταθούν:\n"

#, c-format
msgid "The following packages will be %s:\n"
msgstr "τα ακόλουθα πακέτα θα %s:\n"

#, c-format
msgid "Unable to find all specified slackbuilds.\n"
msgstr "Αδυναμία εύρεσης όλων των slackbuilds.\n"

#, c-format
msgid "Unable to find generated package\n"
msgstr "Αδυναμία εύρεσης του πακέτου που δημιουργήθηκε\n"

#, c-format
msgid "Usage: %s [option(s)] [action] [slackbuild(s)]\n"
msgstr "Χρήση: %s [επιλογή(ες)] [ενέργεια] [slackbuild(s)]\n"

msgid "built"
msgstr "δημιουργηθούν"

msgid "clean build directory"
msgstr "καθαρισμός καταλόγου εργασίας"

msgid "do not look for dependencies"
msgstr "παράλειψη ελέγχου απαιτούμενων πακέτων"

msgid "do not prompt"
msgstr "χωρίς προτροπή"

msgid "fetch, build, and install the specified slackbuild(s)"
msgstr "λήψη, δημιουργία και εγκατάσταση των πακέτων από τα slackbuild"

msgid "fetched"
msgstr "ληφθούν"

msgid "installed"
msgstr "εγκατασταθούν"

msgid "list available slackbuilds"
msgstr "εμφάνιση όλων των διαθέσιμων slackbuild"

msgid "only fetch and build the specified slackbuild(s)"
msgstr "μόνο λήψη και δημιουργία των πακέτων από τα slackbuild"

msgid "only fetch the specified slackbuild(s)"
msgstr "μόνο λήψη των slackbuild(s)"

msgid "run specified command on generated package"
msgstr "εκτέλεση της προσδιοριζόμενης εντολής στο πακέτο που δημιουργείται"

msgid "search available slackbuilds"
msgstr "αναζήτηση στα διαθέσιμα slackbuilds"

msgid "show specified slackbuilds"
msgstr "εμφάνιση ζητούμενο slackbuild"

msgid "update local cache of remote slackbuilds"
msgstr "ενημέρωση τοπικής αποθήκης απομακρυσμένων slackbuilds"

msgid "use the specified configuration file"
msgstr "προσδιορίστε εναλλακτική τοποθεσία για το αρχείο ρυθμίσεων"
