#, c-format
msgid "%s - A SlackBuild utility\n"
msgstr "%s - Nástroj SlackBuild\n\n"

#, c-format
msgid "%s version %s\n"
msgstr "%s verze %s\n"

#, c-format
msgid "Abort.\n"
msgstr "Zrušit.\n"

#, c-format
msgid "BUILD: %s\n"
msgstr "SESTAVENÍ: %s\n"

#, c-format
msgid "Cached\n"
msgstr "Ve vyrovnávací paměti\n"

msgid "Do you want to continue? [y/N] "
msgstr "Chcete pokračovat? [y/N] "

#, c-format
msgid "Done\n"
msgstr "Hotovo\n"

#, c-format
msgid "Download failed: %s\n"
msgstr "Stažení se nezdařilo: %s\n"

#, c-format
msgid "FETCH: %s\n"
msgstr "NAČTENÍ: %s\n"

#, c-format
msgid "Failed\n"
msgstr "Nepodařilo se\n"

#, c-format
msgid "Failed to chdir to %s\n"
msgstr "Nepodařilo se změnit adresář na: %s\n"

msgid "Failed to chdir to build directory"
msgstr "Nepodařilo se změnit adresář na sestavovací adresář"

#, c-format
msgid "Failed to construct command string\n"
msgstr "Nepodařilo se sestavit řetězec příkazů\n"

#, c-format
msgid "Failed to create build directory [%s]\n"
msgstr "Nepodařilo se vytvořit sestavovací adresář [%s]\n"

#, c-format
msgid "Failed to open %s for reading\n"
msgstr "Nepodařilo se otevřít %s pro čtení\n"

#, c-format
msgid "Failed to open current directory\n"
msgstr "Nepodařilo se otevřít aktuální adresář\n"

