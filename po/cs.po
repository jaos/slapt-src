msgid "Missing slackbuild: %s requires %s\n"
msgstr "Chybějících SlackBuild: %s vyžaduje %s\n"

#, c-format
msgid "No BUILDDIR specified in the configuration\n"
msgstr "V konfiguraci není specifikován adresář BUILDDIR\n"

msgid "Options"
msgstr "Možnosti"

#, c-format
msgid "SlackBuild Category: %s\n"
msgstr "Kategorie SlackBuild: %s\n"

#, c-format
msgid "SlackBuild Description: %s\n"
msgstr "Popis SlackBuild: %s\n"

#, c-format
msgid "SlackBuild Files:\n"
msgstr "Soubory SlackBuild:\n"

#, c-format
msgid "SlackBuild Name: %s\n"
msgstr "Název SlackBuild: %s\n"

#, c-format
msgid "SlackBuild Requires: %s\n"
msgstr "SlackBuild vyžaduje: %s\n"

#, c-format
msgid "SlackBuild Version: %s\n"
msgstr "Verze SlackBuild: %s\n"

#, c-format
msgid "The following dependent slackbuilds will be built and installed:\n"
msgstr "Budou sestaveny a nainstalovány tyto závislé slackbuildy:\n"

#, c-format
msgid "The following packages will be %s:\n"
msgstr "Tyto balíčky budou %s:\n"

#, c-format
msgid "Unable to find all specified slackbuilds.\n"
msgstr "Nelze najít všechny specifikované slackbuildy.\n"

#, c-format
msgid "Unable to find generated package\n"
msgstr "Nelze najít vygenerované balíčky\n"

#, c-format
msgid "Usage: %s [option(s)] [action] [slackbuild(s)]\n"
msgstr "Použití: %s [volby] [akce] [slackbuild(y)]\n"

msgid "applicable only to --upgrade-all"
msgstr "použitelné pouze pro volbu --upgrade-all"

#, c-format
msgid "build-only and fetch-only options are mutually exclusive\n"
msgstr "volby build-only a fetch-only se vzájemně vylučují\n"

msgid "built"
msgstr "sestaveno"

msgid "clean build directory"
msgstr "vyprázdnit sestavovací adresář"

msgid "do not look for dependencies"
msgstr "nehledat závislosti"

msgid "do not prompt"
msgstr "neptat se"

msgid "fetch, build, and install the specified slackbuild(s)"
msgstr "načte, sestaví a nainstaluje specifikované slackbuildy"

msgid "fetched"
msgstr "načteno"

msgid "installed"
msgstr "instalováno"

msgid "list available slackbuilds"
msgstr "seznam dostupných SlackBuildů"

msgid "only fetch and build the specified slackbuild(s)"
msgstr "pouze stáhnout a sestavit specifikované slackbuildy"

msgid "only fetch the specified slackbuild(s)"
msgstr "pouze stáhnout specifikované slackbuildy"

msgid "run specified command on generated package"
msgstr "spustit na vygenerovaném balíčku specifikovaný příkaz"

msgid "search available slackbuilds"
msgstr "vyhledat dostupné SlackBuildy"

msgid "show specified slackbuilds"
msgstr "zobrazit uvedené SlackBuildy"

msgid "show what will be done"
msgstr "zobrazit akce, které se provedou"

msgid "update local cache of remote slackbuilds"
msgstr "aktualizovat místní mezipaměť vzdálených SlackBuildů"

msgid "upgrade all installed slackbuilds"
msgstr "upgradovat všechny nainstalované SlackBuildy"

msgid "use the specified configuration file"
msgstr "použít určený konfigurační soubor"
