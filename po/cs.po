msgid "Failed to read %s\n"
msgstr "Nepodařilo se přečíst %s\n"

#, c-format
msgid "Fetching %s..."
msgstr "Načítání %s..."

#, c-format
msgid "Fetching slackbuild list from %s..."
msgstr "Načítání seznamu SlackBuildů z %s..."

#, c-format
msgid "INSTALL: %s\n"
msgstr "INSTALACE: %s\n"

#, c-format
msgid "Individual packages not accepted when upgrading all slackbuilds\n"
msgstr "Při upgradování všech SlackBuildů nejsou jednotlivé balíčky akceptovány\n"

#, c-format
msgid "MD5SUM mismatch for %s\n"
msgstr "MD5SUM pro %s se neshoduje\n"

